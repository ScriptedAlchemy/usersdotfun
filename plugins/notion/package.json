{
  "name": "@curatedotfun/notion",
  "version": "0.0.7",
  "description": "Notion plugin for curatedotfun",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/potlock/curatedotfun-plugins.git",
    "directory": "packages/notion"
  },
  "keywords": [
    "curatedotfun",
    "notion",
    "distribute",
    "plugin"
  ],
  "scripts": {
    "build": "rspack build && tsc -p tsconfig.build.json",
    "dev": "rspack serve",
    "lint": "tsc --noEmit",
    "test": "vitest"
  },
  "devDependencies": {
    "@curatedotfun/utils": "^0.0.2",
    "@module-federation/node": "^2.7.10",
    "@rspack/cli": "latest",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.7"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "effect": "^3.17.6",
    "zod": "^4.0.8"
  },
  "dependencies": {
    "@usersdotfun/core-sdk": "workspace:*",
    "@notionhq/client": "^2.2.15"
  }
}
