      "type": "object",
      "properties": {
        "variables": {
          "type": "object",
          "properties": {
            "template": {
              "type": "string"
            }
          },
          "required": [
            "template"
          ],
          "additionalProperties": false
        },
        "secrets": {
          "type": "object",
          "propertyNames": {
            "type": "string"
          },
          "additionalProperties": {}
        }
      },
      "additionalProperties": false
    },
    "inputSchema": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "type": "object",
