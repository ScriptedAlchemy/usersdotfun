{
  "name": "@usersdotfun/shared-types",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "type": "module",
  "private": true,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./schemas": {
      "types": "./src/schemas/index.ts",
      "default": "./src/schemas/index.ts"
    },
    "./schemas/*": {
      "types": "./src/schemas/*.ts",
      "default": "./src/schemas/*.ts"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "default": "./src/types/index.ts"
    },
    "./types/*": {
      "types": "./src/types/*.ts",
      "default": "./src/types/*.ts"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "default": "./src/utils/index.ts"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "start": "bun src/index.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "cron-parser": "^5.3.0",
    "zod": "^4.0.10"
  }
}
