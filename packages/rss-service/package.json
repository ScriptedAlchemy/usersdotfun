{
  "name": "@curatedotfun/rss-service",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./api": {
      "import": "./dist/api.js",
      "types": "./dist/api.d.ts"
    }
  },
  "scripts": {
    "build": "bun build ./src/index.ts ./src/api.ts --outdir=dist --target=bun && tsc --emitDeclarationOnly -p tsconfig.json",
    "dev": "PORT=4001 bun --hot src/index.ts",
    "start": "bun dist/index.js"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.2",
    "effect": "^3.17.7",
    "feed": "^5.1.0",
    "hono": "^4.8.8",
    "ioredis": "^5.7.0",
    "uuid": "^10.0.0",
    "zod": "^4.0.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.7.2"
  }
}
