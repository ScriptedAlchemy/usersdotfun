{
  "jobs": [
    {
      "id": "open crosspost",
      "name": "Fetch features from open crosspost",
      "schedule": "* * * * *",
      "source": {
        "plugin": "@curatedotfun/masa-source",
        "config": {
          "secrets": {
            "apiKey": "0afdwAl4m5hj1q9AGUQnHMRPQuuYVKD3T4RVlkgGe6KNe46y"
          }
        },
        "search": {
          "type": "twitter-scraper",
          "language": "en",
          "platformArgs": {
            "hashtags": ["feature"],
            "fromAccounts": ["open_crosspost"]
          }
        }
      },
      "pipeline": {
        "id": "standard-tweet-processing",
        "steps": [
          {
            "pluginName": "@curatedotfun/simple-transform",
            "config": { "variables": { "template": "New Tweet from {{Metadata.author}}: {{Content}}" } },
            "stepId": "format-tweet"
          }
        ]
      }
    }
  ]
}
